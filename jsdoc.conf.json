{
    "opts": {
        "destination": "latest/apidocs"
    },
    "source": {
        "include": [
            "code/valerie/documentation-extras.js",
            "code/valerie/core",
            "code/valerie/full",
            "code/valerie/localisation/en",
            "code/valerie/localisation/en-gb",
            "code/valerie/localisation/en-us"
        ],
        "includePattern": ".+\\.js(doc)?$"
    },
    "templates": {
        "default": {
            "outputSourceFiles": true
        },
        "systemName": "valerie",
        "footer": "A validation library for KnockoutJS.",
        "copyright": "&copy; egrove Ltd 2013.",
        "navType": "inline",
        "theme": "spacelab",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": false
    }
}