{
    "opts": {
        "destination": "apiDocs"
    },
    "source": {
        "include": [
            "lib/documentation-extras.js",
            "lib/core",
            "lib/full",
            "lib/localisation/en",
            "lib/localisation/en-gb",
            "lib/localisation/en-us"
        ],
        "includePattern": ".+\\.js(doc)?$"
    },
    "templates": {
